 int num1 = Convert.ToInt32(nums1.Text);
 int num2 = Convert.ToInt32(nums2.Text);
 int num3 = Convert.ToInt32(nums3.Text);

 if (num1 > num2 && num1 > num3)
 {
     result.Text="Num1 is the largest" ;
 }
 else if (num2 > num1 && num2 > num3)
 {
     result.Text=("Num2 is the largest");
 }
 else
 {
     result.Text=("Num3 is the largest");
 }
